<participant>

<!-- ASP/JavaScript ---------------------------------------------------------->
  <implementation serverModel="ASP/JavaScript">
     <insertText location="afterSelection">
<![CDATA[
  <form method="post" action="@@formAction@@" name="@@formName@@">
    @@formContent@@
  </form><p>&nbsp;</p>]]>
