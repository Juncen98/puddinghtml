<participant>

<!-- ASP/JavaScript ---------------------------------------------------------->
  <implementation serverModel="ASP/JavaScript">
     <insertText location="afterSelection">
<![CDATA[<table align="@@tableAlign@@" border="@@border@@">
    @@tableContent@@
  </table><br>]]>
     </insertText>
  </implementation>
  
