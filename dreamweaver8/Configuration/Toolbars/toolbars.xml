			file="Toolbars/MM/ViewOptionsMenu.htm"
			update="onViewChange"/>


        <menubutton id="DW_VisualAids"
            image="Toolbars/images/MM/T_VisualAids_Lg_N.png"
            disabledImage="Toolbars/images/MM/T_VisualAids_Lg_D.png"
            enabled="dw.getDocumentDOM() != null && (dw.getDocumentDOM().getView() == 'split' || dw.getDocumentDOM().getView() == 'design')"
            tooltip="可视化助理"
