			menuID="DWDesignVisualAidsPopup"/>

	</toolbar>
	
    <!-- Standard toolbar -->

    <toolbar id="Standard_Toolbar" label="标准" initiallyVisible="false" backgroundStyle="gradient">


        <button id="DW_New"
