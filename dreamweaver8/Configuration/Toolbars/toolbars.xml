<?xml version="1.0"?>
<!DOCTYPE toolbarset SYSTEM "-//Macromedia//DWExtension toolbar 5.0">
<toolbarset>
    <!-- main toolbar -->

    <toolbar id="DW_Toolbar_Main" label="文档" backgroundStyle="gradient">


	   <radiobutton id="DW_CodeView"
            image="Toolbars/images/MM/codeView.png"
            disabledImage="Toolbars/images/MM/codeView_dis.png"
            imageMac="dwres:18055"
            disabledImageMac="dwres:18078"
            tooltip="显示代码视图"
			domRequired="FALSE"
			label="代码"
			buttonGroup="Document View"
            enabled="dw.getDocumentDOM() != null"
            checked="dw.getDocumentDOM() != null && dw.getDocumentDOM().getView() == 'code'"
            command="dw.getDocumentDOM().setView('code')"
            update="onViewChange"/>


        <radiobutton id="DW_SplitView"
            image="Toolbars/images/MM/splitView.png"
            disabledImage="Toolbars/images/MM/splitView_dis.png"
            imageMac="dwres:18054"
            disabledImageMac="dwres:18079"
            tooltip="显示代码视图和设计视图"
			label="拆分"
			domRequired="FALSE"
			buttonGroup="Document View"
            enabled="dw.getDocumentDOM() != null && dw.canShowDesignView(dw.getDocumentDOM())"
            checked="dw.getDocumentDOM() != null && dw.getDocumentDOM().getView() == 'split'"
            command="dw.getDocumentDOM().setView('split')"
            update="onViewChange"/>


        <radiobutton id="DW_DesignView"
            image="Toolbars/images/MM/designView.png"
            disabledImage="Toolbars/images/MM/designView_dis.png"
            imageMac="dwres:18053"
            disabledImageMac="dwres:18077"
			label="设计"
            tooltip="显示设计视图"
			domRequired="FALSE"
			buttonGroup="Document View"
            enabled="dw.getDocumentDOM() != null && dw.canShowDesignView(dw.getDocumentDOM())"
            checked="dw.getDocumentDOM() != null && dw.getDocumentDOM().getView() == 'design'"
            command="dw.getDocumentDOM().setView('design')"
            update="onViewChange"/>


        <checkbutton id="DW_LiveDataView"
            image="Toolbars/images/MM/liveDataView.png"
            disabledImage="Toolbars/images/MM/liveDataView_dis.png"
            imageMac="dwres:18087"
            disabledImageMac="dwres:18088"
            tooltip="动态数据视图"
		    enabled="!site.serverActivity() && dw.getDocumentDOM() != null && dw.canShowDesignView(dw.getDocumentDOM())"
			checked="dw.getLiveDataMode()"
			command="dw.setLiveDataMode(!dw.getLiveDataMode())"
			showIf="dw.getDocumentDOM() && dw.getDocumentDOM().getServerNameFromDoc() != '' && dw.getDocumentDOM().getServerNameFromDoc() != 'XSLT'"
            update="onViewChange,onLiveDataModeChange"/>


        <checkbutton id="DW_LiveDebug"
			platform="win"
            image="Toolbars/images/MM/liveDebug.png"
            disabledImage="Toolbars/images/MM/liveDebug_dis.png"
            imageMac="dwres:773"
            disabledImageMac="dwres:18063"
            tooltip="服务器调试"
            enabled="dw.getDocumentDOM() != null && dw.canLiveDebug() && dw.canShowDesignView(dw.getDocumentDOM())"
            checked="dw.getDocumentDOM() != null && dw.getDocumentDOM().getView() == 'browse'"
            command="dw.toggleLiveDebug()"
			showIf="dw.getDocumentDOM() != null && dw.canLiveDebug()"
	        update="onEveryIdle"/>


        <editcontrol id="DW_SetTitle"
            label="   标题: "
            tooltip="文档标题"
            width="150"
            file="Toolbars/MM/EditTitle.htm"/>


        <menubutton id="DW_ValidatorErrors"
            image="Toolbars/images/MM/validatornoerrors.png"
            disabledImage="Toolbars/images/MM/validator_dis.png"
            imageMac="dwres:6319"
            disabledImageMac="dwres:6320"
			arguments="'valmenu'"
            tooltip="浏览器目标检查"
			file="Toolbars/MM/ValidatorErrors.htm"
            update="onViewChange,onValidationChange"/>


        <menubutton id="DW_Validator"
            image="Toolbars/images/MM/T_Validator_Lg_N.png"
            disabledImage="Toolbars/images/MM/T_Validator_Lg_D.png"
            tooltip="验证标记"
			menuID="DWValidateTabStart" 
            enabled="dw.getDocumentDOM() != null"
            update="onViewChange"/>


        <menubutton id="DW_FileTransfer"
            image="Toolbars/images/MM/fileTransfer.png"
            disabledImage="Toolbars/images/MM/fileTransfer_dis.png"
            imageMac="dwres:18056"
            disabledImageMac="dwres:6316"
            tooltip="文件管理"
            enabled="dw.getDocumentDOM() != null"
            menuID="DWDocStatePopup"/>


        <menubutton id="DW_Preview"
            image="Toolbars/images/MM/preview.png"
            disabledImage="Toolbars/images/MM/preview_dis.png"
            imageMac="dwres:18058"
            disabledImageMac="dwres:18071"
            tooltip="在浏览器中预览/调试"
            enabled="dw.getDocumentDOM() != null"
            menuID="DWPIBPopup"/>

        <separator/>

        <button id="DW_DocRefresh"
            image="Toolbars/images/MM/refresh.png"
            disabledImage="Toolbars/images/MM/refresh_dis.png"
            imageMac="dwres:18059"
            disabledImageMac="dwres:18074"
            tooltip="刷新设计视图 (F5)"
            enabled="((dw.getDocumentDOM() != null) && (dw.getDocumentDOM().getView() != 'browse') && (!dw.getDocumentDOM().isDesignViewUpdated()))"
            command="dw.getDocumentDOM().synchronizeDocument()"
            update="onViewChange,onCodeViewSyncChange"/>

