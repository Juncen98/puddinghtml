<html>
<head>
<!-- TemplateBeginEditable name="doctitle" -->
<title>Untitled Document</title>
<!-- TemplateEndEditable -->
<meta http-equiv="Content-Type" content="text/html; charset=">
<!-- TemplateBeginEditable name="head" -->
<!-- TemplateEndEditable -->
<!-- TemplateParam name="FormName" type="text" value="form1" -->
<!-- TemplateParam name="FormMethod" type="text" value="post" -->
<!-- TemplateParam name="FormAction" type="text" value="" -->
</head>
<body class="sub">
<form name="FormName" method="@@(FormMethod)@@" action="@@(FormAction)@@">
  <table width="90%" border="0" cellspacing="0" cellpadding="1" class="TitleColor">
    <tr>
      <td>
        <table width="100%" border="0" cellspacing="0" cellpadding="4">
          <tr class="HeaderColor">
            <td colspan="3">
              <!-- TemplateBeginEditable name="Title" -->
              <h3>Search </h3>
              <!-- TemplateEndEditable -->
            </td>
          </tr>
          <tr>
            <td style="text-align: right" class="TitleColor">
              <!-- TemplateBeginEditable name="Keywords" -->
              <label for="keywords">Keywords: </label>
              <!-- TemplateEndEditable -->
            </td>
            <td class="TitleColor">
              <!-- TemplateBeginEditable name="Keywords2" -->
              <input size="50" id="keywords" name="textfield" value="">
              <!-- TemplateEndEditable -->
            </td>
            <td rowspan="4" style="vertical-align: middle">
              <!-- TemplateBeginEditable name="Navigation" -->
              <p>&#149; <a href="#">Lorem</a></p>
              <p>&#149; <a href="#">Ipsum</a></p>
              <p>&#149; <a href="#">Dolor</a></p>
              <!-- TemplateEndEditable -->
            </td>
          </tr>
          <tr>
            <td style="text-align: right" class="TitleColor">
              <!-- TemplateBeginEditable name="Options" --> Options: <!-- TemplateEndEditable -->
            </td>
            <td class="TitleColor">
              <!-- TemplateBeginEditable name="Options2" -->
              <label for="option1">Lorem </label>
              <input type="radio" id="option1" name="radiobutton" value="radiobutton" checked>
              <label for="option2">Ipsum </label>
              <input type="radio" id="option2" name="radiobutton" value="radiobutton">
              <label for="option3">Dolor </label>
              <input type="radio" id="option3" name="radiobutton" value="radiobutton">
              <!-- TemplateEndEditable -->
            </td>
          </tr>
          <tr>
            <td style="text-align: right" class="TitleColor">
              <label for="select"><!-- TemplateBeginEditable name="Language" --> Language:<!-- TemplateEndEditable --></label>
            </td>
            <td class="TitleColor">
              <!-- TemplateBeginEditable name="Language2" -->
              <select id="select" name="select">
                <option selected>Select a Language</option>
              </select>
              <!-- TemplateEndEditable -->
            </td>
          </tr>
          <tr>
            <td>&nbsp;
            </td>
            <td>
              <!-- TemplateBeginEditable name="Buttons" -->
              <input type="submit" name="Submit" value="Go">
              <!-- TemplateEndEditable -->
            </td>
          </tr>
        </table>
      </td>
    </tr>
  </table>
</form>
</body>
</html>
