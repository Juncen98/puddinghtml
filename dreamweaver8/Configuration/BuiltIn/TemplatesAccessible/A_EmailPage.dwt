<!-- TemplateParam name="BCC" type="boolean" value="true" -->
</head>
<body class="sub">
<form name="@@(FormName)@@" method="@@(FormMethod)@@" action="@@(FormAction)@@">
  <table width="90%" border="0" cellspacing="2" cellpadding="4">
    <tr>
      <td colspan="3" style="vertical-align: top" width="33%" class="HeaderColor">
        <!-- TemplateBeginEditable name="Title" -->
        <h3>Email Page</h3>
        <!-- TemplateEndEditable -->
      </td>
    </tr>
    <tr>
      <td width="40%" style="vertical-align: top" class="StoryContentColor">
        <!-- TemplateBeginEditable name="Instructions" -->
        <h4>Instructions:</h4>
        Lorem ipsum dolor sit amet, consectetur adipiscing elit. Lorem ipsum dolor sit amet, consectetur adipiscing elit. Lorem ipsum dolor sit amet, consectetur adipiscing elit.<!-- TemplateEndEditable -->
      </td>
      <td style="text-align: right; vertical-align: top">
        <table width="100%" border="0" cellspacing="0" cellpadding="0">
          <!-- TemplateBeginIf cond="To" -->
          <tr>
            <td width="79%" style="text-align: right; vertical-align: top">
              <!-- TemplateBeginEditable name="To" -->
              <label for="to" class="TitleColor"><strong>To:</strong></label>
              <input id="to" name="to" type="text" size="50">
              <!-- TemplateEndEditable -->
            </td>
          </tr>
          <!-- TemplateEndIf --> <!-- TemplateBeginIf cond="CC" -->
          <tr>
            <td style="text-align: right; vertical-align: top">
              <!-- TemplateBeginEditable name="CC" -->
              <label for="cc" class="TitleColor"><strong>cc:</strong></label>
              <input id="cc" name="cc" type="text" size="50">
              <!-- TemplateEndEditable -->
            </td>
          </tr>
          <!-- TemplateEndIf --><!-- TemplateBeginIf cond="BCC" -->
          <tr>
            <td style="text-align: right; vertical-align: top">
              <!-- TemplateBeginEditable name="BCC" -->
              <label for="bcc"  class="TitleColor"><strong>bcc:</strong></label>
              <input id="bcc" name="bcc" type="text" size="50">
              <!-- TemplateEndEditable -->
            </td>
          </tr>
          <!-- TemplateEndIf -->
          <tr>
            <td style="text-align: right; vertical-align: top">
              <!-- TemplateBeginEditable name="From" -->
              <label for="from" class="TitleColor"><strong>from:</strong></label>
              <input id="from" name="from" type="text" size="50">
              <!-- TemplateEndEditable -->
            </td>
          </tr>
          <tr>
            <td style="text-align: right; vertical-align: top">
              <!-- TemplateBeginEditable name="Subject" -->
              <label for="subject" class="TitleColor"><strong>subject:</strong></label>
              <input id="subject" name="subject" type="text" size="50">
              <!-- TemplateEndEditable -->
