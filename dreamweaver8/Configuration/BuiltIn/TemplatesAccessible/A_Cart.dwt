<html>
<head>
<!-- TemplateBeginEditable name="doctitle" -->
<title>Untitled Document</title>
<!-- TemplateEndEditable -->
<meta http-equiv="Content-Type" content="text/html; charset=">
<!-- TemplateBeginEditable name="head" -->
<!-- TemplateEndEditable -->
<!-- TemplateParam name="ShowHorizontalRule" type="boolean" value="true" -->
<!-- TemplateParam name="FormMethod" type="text" value="post" -->
<!-- TemplateParam name="FormAction" type="text" value="" -->
<!-- TemplateParam name="FormName" type="text" value="form1" -->
<!-- TemplateParam name="src" type="text" value="" -->
<!-- TemplateParam name="alt" type="text" value="image" -->
<!-- TemplateParam name="name" type="text" value="image" -->
<!-- TemplateParam name="width" type="text" value="30" -->
<!-- TemplateParam name="height" type="text" value="30" -->
</head>
<body class="sub">
<form name="@@(FormName)@@" method="@@(FormMethod)@@" action="@@(FormAction)@@">
  <table style="text-align: center" width="100%" border="0" cellspacing="0" cellpadding="4">
    <tr style="vertical-align: top" class="HeaderColor">
      <td colspan="2">
        <!-- TemplateBeginEditable name="headerTitle" -->Title<!-- TemplateEndEditable -->
      </td>
      <td style="text-align: center">
        <!-- TemplateBeginEditable name="headerQuantity" -->Quantity<!-- TemplateEndEditable -->
      </td>
      <td style="text-align: center">
        <!-- TemplateBeginEditable name="headerPrice" -->Price<!-- TemplateEndEditable -->
      </td>
      <td style="text-align: center">
        <!-- TemplateBeginEditable name="headerAction" -->Action<!-- TemplateEndEditable -->
      </td>
    </tr>
    <!-- TemplateBeginRepeat name="item" --> <!-- TemplateBeginIf cond="ShowHorizontalRule" -->
    <tr style="vertical-align: top">
      <td style="text-align: center" colspan="5">
        <!-- TemplateBeginEditable name="HorizontalRule" -->
        <hr noshade>
        <!-- TemplateEndEditable -->
      </td>
    </tr>
    <!-- TemplateEndIf -->
    <tr style="vertical-align: top">
      <td width="5%" style="text-align: center">
        <img src="@@(src)@@" alt="@@(alt)@@" name="@@(name)@@" width="@@(width)@@" height="@@(height)@@">
      </td>
      <td width="55%">
        <!-- TemplateBeginEditable name="description" --><a href="#">Lorem ipsum dolor</a><br>
        Lorem ipsum dolor sit amet, consectetur adipiscing elit. <!-- TemplateEndEditable -->
      </td>
      <td width="10%" style="text-align: center" class="FooterColor">
        <!-- TemplateBeginEditable name="quantity" -->
        <label for="textfield">Save:
        <input id="textfield" type="text" size="5" maxlength="4" value="1">
        </label>
        <!-- TemplateEndEditable -->
      </td>
      <td width="15%" style="text-align: right" class="FooterColor">
        <!-- TemplateBeginEditable name="price" -->$10.00<!-- TemplateEndEditable -->
      </td>
      <td width="15%" style="text-align: center">
        <!-- TemplateBeginEditable name="action" -->
        <input type="submit" name="Submit1" value="Save">
        <br>
        <input type="submit" name="Submit2" value="Delete">
        <!-- TemplateEndEditable -->
      </td>
    </tr>
    <!-- TemplateEndRepeat -->
    <tr style="vertical-align: middle" class="FooterColor">
      <td width="60%" style="text-align: right" colspan="2">
        <!-- TemplateBeginEditable name="footerTotal" --> Total:<!-- TemplateEndEditable -->
      </td>
      <td width="10%" style="text-align: center">
        <!-- TemplateBeginEditable name="footerQuantity" -->1<!-- TemplateEndEditable -->
      </td>
      <td width="15%" style="text-align: right">
        <!-- TemplateBeginEditable name="footerPrice" -->$20.50<!-- TemplateEndEditable -->
      </td>
      <td width="15%" style="text-align: center">
        <!-- TemplateBeginEditable name="footerAction" -->
        <input type="submit" name="Submit2" value="Buy">
        <!-- TemplateEndEditable -->
      </td>
    </tr>
  </table>
</form>
</body>
</html>
