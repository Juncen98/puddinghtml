<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<panelset>

	<document maximize="false" rect="0 58 786 1000"/>
	
	<!-- SECOND MONITOR RIGHT - Code Inspector -->
  <panelframe dock="floating" monitor="2" height="700" width="600" x="right" y="bottom">
    <panelcontainer expanded="true" height="700" visible="true">
      <panel id="html"/>
    </panelcontainer>
  </panelframe>

	<!-- MAIN TOP - InsertBar -->
	<panelframe monitor="1" x="left" y="top" width="659" height="34" >
		<panelcontainer>
			<panel id="objects"/>
		</panelcontainer>
	</panelframe>

	<!-- SECOND MONITOR BOTTOM - Property Inspector -->
	<panelframe monitor="2" x="left" y="bottom" width="767" height="102">
		<panelcontainer>
			<panel id="properties"/>
		</panelcontainer>
	</panelframe>

 	<!-- SECOND MONITOR LEFT - Site Maximize -->
	<panelframe height="460" visible="true" width="650" monitor="2" x="right" y="top">
		<panelcontainer expanded="true" height="460" maxRestoreGroup="Files" maxRestoreIndex="0" maxRestorePanel="assets" maximize="true" title="Files" visible="true">
			<panel id="site"/>
		</panelcontainer>
	</panelframe>

	<!-- SECOND MONITOR RIGHT - CSS(Design), Code, Application, Tag Inspector, Frames (hidden), History (hidden) -->
	<panelframe monitor="2" x="left" y="top" width="300">
		<panelcontainer expanded="false" title="CSS">
      		<panel id="css styles"/>
      		<panel id="layers"/>
		</panelcontainer>
		<panelcontainer expanded="false" title="Application" >
      		<panel id="database"/>
      		<panel id="data sources"/>
      		<panel id="server scripts"/>
      		<panel id="server components"/>
		</panelcontainer>
		<panelcontainer expanded="false">
			<panel id="tag inspector"/>
		</panelcontainer>
		<panelcontainer expanded="false">
			<panel id="assets"/>
			<panel id="snippets"/>
		</panelcontainer>
		<panelcontainer visible="false" >
			<panel id="frames"/>
		</panelcontainer>
		<panelcontainer visible="false" >
			<panel id="history"/>
		</panelcontainer>
	</panelframe>

	<!-- FLOATING in middle, individuals don't need to be listed (default behavior) but list groups-->
	<panelframe x="100" y="400" width="700">
		<panelcontainer title="Results" visible="false">
      		<panel id="search"/>
 			<panel id="reference"/>
      		<panel id="validation"/>
      		<panel id="btc"/>
      		<panel id="linkchecker"/>
      		<panel id="reports"/>
      		<panel id="ftp log"/>
		</panelcontainer>
	</panelframe>

	<!-- FLOATING, individuals don't need to be listed - default behavior...-->
</panelset>
