<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<panelset>

	<document maximize="false" rect="0 58 786 700"/>

	<!-- MAIN TOP - InsertBar -->
	<panelframe monitor="1" x="left" y="top" width="767" height="34" >
		<panelcontainer>
			<panel id="objects"/>
		</panelcontainer>
	</panelframe>

	<!-- MAIN BOTTOM - Property Inspector -->
	<panelframe monitor="1" x="left" y="bottom" width="767" height="102">
		<panelcontainer>
			<panel id="properties"/>
		</panelcontainer>
	</panelframe>

	<!-- MAIN RIGHT - Design, Code, Application, Tag Inspector, Files, Frames (hidden), History (hidden) -->
	<panelframe monitor="1" x="right" y="top" height="800" >
		<panelcontainer expanded="false" title="CSS" >
      		<panel id="css styles"/>
      		<panel id="layers"/>
		</panelcontainer>
		<panelcontainer expanded="false" title="Application" >
      		<panel id="database"/>
      		<panel id="data sources"/>
      		<panel id="server scripts"/>
      		<panel id="server components"/>
		</panelcontainer>
		<panelcontainer expanded="false" >
			<panel id="tag inspector"/>
		</panelcontainer>
		<panelcontainer title="Files" >
			<panel id="site"/>
			<panel id="assets"/>
			<panel id="snippets"/>
		</panelcontainer>
		<panelcontainer visible="false" >
			<panel id="frames"/>
		</panelcontainer>
		<panelcontainer visible="false" >
			<panel id="history"/>
		</panelcontainer>
	</panelframe>

	<!-- FLOATING in middle, individuals don't need to be listed (default behavior) but list groups-->
	<panelframe x="100" y="400" width="700">
		<panelcontainer title="Results" visible="false">
      		<panel id="search"/>
 			<panel id="reference"/>
      		<panel id="validation"/>
      		<panel id="btc"/>
      		<panel id="linkchecker"/>
      		<panel id="reports"/>
      		<panel id="ftp log"/>
		</panelcontainer>
	</panelframe>
</panelset>
