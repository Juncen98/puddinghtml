      <attriboption value="application/rtf" caption="application/rtf"/>
      <attriboption value="video/x-msvideo" caption="video/x-msvideo"/>
      <attriboption value="video/quicktime" caption="video/quicktime"/>
      <attriboption value="video/x-mpeg2" caption="video/x-mpeg2"/>
      <attriboption value="audio/x-pn/realaudio" caption="audio/x-pn/realaudio"/>
      <attriboption value="audio/x-mpeg" caption="audio/x-mpeg"/>
