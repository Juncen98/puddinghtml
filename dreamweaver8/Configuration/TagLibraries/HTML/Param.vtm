      <attriboption value="image/x-portablebitmap" caption="image/x-portablebitmap"/>
		</attrib>
    <attrib name="ID"/>
	</attributes>
	<attribcategories>
		<attribgroup name="General" elements="name,value" />
		<attribgroup name="HTML 4.0" elements="valuetype,type" />
