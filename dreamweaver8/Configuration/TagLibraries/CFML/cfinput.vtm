          <ATTRIBOPTION value="2" />
		  <ATTRIBOPTION value="3" />
          <ATTRIBOPTION value="4" />
		  <ATTRIBOPTION value="5" />
		  <ATTRIBOPTION value="6" />
      </ATTRIB>
