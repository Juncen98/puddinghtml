      <ATTRIBGROUP NAME="General" ELEMENTS="NAME,TYPE,VALUE"/>
      <ATTRIBGROUP NAME="Appearance" ELEMENTS="HEIGHT,WIDTH,SIZE,MAXLENGTH"/>
      <ATTRIBGROUP NAME="Special" ELEMENTS="VALIDATE,RANGE,REQUIRED,MESSAGE,CHECKED,NOTAB,TABINDEX,ONVALIDATE,PATTERN,PASSTHROUGH,BIND,LABEL,MASK,VALIDATEAT,SRC,DAYNAMES,MONTHNAMES,FIRSTDAYOFWEEK"/>
 	  <ATTRIBGROUP NAME="Events" ELEMENTS="ONERROR,ONVALIDATE,ONKEYUP,ONKEYDOWN,ONMOUSEUP,ONMOUSEDOWN,ONCHANGE,ONCLICK"/>
      <ATTRIBGROUP NAME="Misc" ELEMENTS=""/>
