	  <ATTRIB NAME="MASK" TYPE="TEXT"/>
	  <ATTRIB NAME="VALIDATEAT" TYPE="ENUMERATED">
	  	 <ATTRIBOPTION VALUE="onSubmit"/>
         <ATTRIBOPTION VALUE="OnServer"/>
		 <ATTRIBOPTION VALUE="onBlur"/>
      </ATTRIB>
	  <ATTRIB NAME="VALIDATE" TYPE="ENUMERATED">
          <ATTRIBOPTION value="" />
