<tag name="cfxml" endtag="yes">
<tagformat nlbeforetag="1" nlaftertag="1"/>
<tagdialog file="cfxml.htm"/>
  <attributes>
    <attrib name="casesensitive" type="ENUMERATED">
      <attriboption value="yes"/>
      <attriboption value="no"/>
    </attrib>
    <attrib name="variable" type="TEXT"/>
   </attributes>
	<attribcategories>
		<attribgroup name="General" elements="variable,casesensitive" />
	</attribcategories>
</tag>
