<tag name="attribute-set" endtag="xml" tagtype="nonempty" casesensitive="yes">
<tagformat nlbeforetag="1" nlbeforecontents="1" nlaftercontents="1" nlaftertag="1" indentcontents="yes"/>
  <attributes>
    <attrib name="name"								 type="TEXT" casesensitive="yes"/>
    <attrib name="use-attribute-sets"  type="TEXT"  casesensitive="yes"/>
  </attributes>
  <attribcategories>
    <attribgroup name="General" elements="name,use-attribute-sets" />
  </attribcategories>
</tag>
