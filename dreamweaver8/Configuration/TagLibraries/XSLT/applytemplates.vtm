<tag name="apply-templates" endtag="xml" tagtype="nonempty" casesensitive="yes">
<tagformat nlbeforetag="1" nlbeforecontents="1" nlaftercontents="1" nlaftertag="1" indentcontents="yes"/>
  <attributes>
    <attrib name="select" type="XPATH" casesensitive="yes"/>
    <attrib name="mode"  type="TEXT"  casesensitive="yes"/>
  </attributes>
  <attribcategories>
    <attribgroup name="General" elements="select,mode" />
  </attribcategories>
</tag>
