<participant version="4.0">
	<insertText location="afterSelection"><![CDATA[<%= IIf((@@RecordsetName@@.RecordCount = 0), 0, (@@RecordsetName@@.StartRecord + 1)) %>]]></insertText>
	<searchPatterns whereToSearch="directive">
		<searchPattern paramNames="RecordsetName" isOptional="false" limitSearch="all"><![CDATA[/<%= IIf\(\(([^\r\n]*?)\.RecordCount\s*=\s*0\),\s*0,\s*\([^\r\n]*?\.StartRecord\s*\+\s*1\)\) %>/i]]></searchPattern>
	</searchPatterns>
	<quickSearch>.RecordCount</quickSearch>
</participant>
